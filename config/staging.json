{
    "port": "9009",
    "log": {
        "level": "info"
    },
    "paginate": {
        "default": 10,
        "max": 50
    },
    "authentication": {
        "secret": "",
        "jwt": {
            "header": {
                "typ": "access"
            },
            "audience": "https://yourdomain.com",
            "subject": "anonymous",
            "issuer": "your-project",
            "algorithm": "HS256",
            "expiresIn": "1d"
        },
        "cookie": {
            "enabled": true,
            "name": "your-project-jwt",
            "httpOnly": false,
            "secure": false
        }
    },
    "mongodb": {
        "uri": "mongodb://localhost:27017/nodejs-base-project"
    },
    "raven": {
        "enable": false,
        "client_id": ""
    },
    "agenda": {
        "db": {
            "address": "mongodb://localhost:27017/nodejs-base-project",
            "collection": "jobs"
        },
        "job_types": ""
    }
}
